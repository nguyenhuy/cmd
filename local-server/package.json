{
	"name": "local-server",
	"version": "0.0.1",
	"license": "UNLICENSED",
	"type": "module",
	"main": "./dist/tsc/src/main.js",
	"types": "./dist/tsc/src/main.d.ts",
	"browser": "./dist/esbuild/browser.js",
	"bin": {
		"my-cli-tool": "./dist/tsc/src/main.js"
	},
	"scripts": {
		"run": "tsx ./src/main.js",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"lint:show-config": "eslint --print-config",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test:debug": "node --experimental-vm-modules --inspect node_modules/jest/bin/jest.js",
		"test:watch": "yarn test --watch",
		"test:coverage": "yarn test --coverage",
		"clean": "rm -rf dist build package",
		"tsx": "tsx",
		"docs": "typedoc --entryPoints src/main.ts",
		"compile": "tsc -p tsconfig.json",
		"compile:watch": "yarn compile --watch",
		"build": "node ./build.js",
		"build:prod": "NODE_ENV=production yarn build && yarn copy-to-app",
		"build:watch": "yarn build --watch",
		"copy-to-app": "yarn tsx ./tools/copy-to-app.ts",
		"watch": "yarn build:watch & yarn tsx ./tools/pkg-watch.ts",
		"hot-reload-server": "yarn tsx ./tools/hot-server-reload.ts",
		"export-schema-swift": "yarn tsx ./tools/generateSwiftSchema.ts"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^1.2.12",
		"@ai-sdk/openai": "^1.3.22",
		"@openrouter/ai-sdk-provider": "^0.5.0",
		"ai": "^4.3.16",
		"express": "^4.21.2",
		"express-promise-router": "^4.1.1",
		"fzf": "^0.5.2",
		"globby": "^14.1.0",
		"openai": "^4.83.0",
		"p-wait-for": "^5.0.2",
		"simple-git": "^3.27.0",
		"tslib": "^2.8.1",
		"uuid": "^11.1.0",
		"vscode-material-icons": "^0.1.1",
		"ws": "^8.18.1",
		"zod": "^3.25.30"
	},
	"devDependencies": {
		"@eslint/js": "^9.19.0",
		"@types/express": "^5.0.0",
		"@types/express-promise-router": "^3.0.2",
		"@types/handlebars": "^4.1.0",
		"@types/jest": "^29.5.0",
		"@types/nock": "^11.1.0",
		"@types/node": "^22.12.0",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^10.0.0",
		"@types/ws": "^8.5.14",
		"@typescript-eslint/eslint-plugin": "^8.22.0",
		"@typescript-eslint/parser": "^8.22.0",
		"dedent-js": "^1.0.1",
		"esbuild": "^0.24.2",
		"esbuild-plugin-tsc": "^0.4.0",
		"eslint": "^9.19.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.4.0",
		"eslint-plugin-react": "^7.37.4",
		"fs-extra": "^11.3.0",
		"handlebars": "^4.7.8",
		"jest": "^29.5.0",
		"jest-file-snapshot": "^0.7.0",
		"json-schema": "^0.4.0",
		"nock": "^14.0.1",
		"node-watch": "^0.7.4",
		"prettier": "^3.4.2",
		"supertest": "^7.0.0",
		"superwstest": "^2.0.4",
		"ts-jest": "^29.1.0",
		"ts-json-schema-generator": "^2.3.0",
		"tsx": "^4.19.2",
		"typedoc": "^0.27.6",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.22.0",
		"yargs": "^17.7.2"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
